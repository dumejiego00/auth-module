{
    "version": 2,
    "builds": [
      {
        "src": "app.ts",
        "use": "@vercel/node",
        "config": {
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/dist/$1"
      }
    ],
    "buildCommand": "npm run build:tsc"
  }
  