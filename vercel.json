{
    "version": 2,
    "builds": [
      {
        "src": "app.ts",
        "use": "@vercel/node"
      }
    ],
    "build": {
      "env": {
        "NODE_ENV": "production"
      },
      "commands": {
        "build": "npm run build:tsc"
      }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/dist/$1"
      }
    ]
  }
  